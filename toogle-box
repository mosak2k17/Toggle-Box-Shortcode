function toggle_shortcode( $atts, $content = null ) {
extract( shortcode_atts(
array(
'title' => 'Click To Open',
'color' => ''
),
$atts ) );
return '<h3><a href="#">'. $title .'</a></h3><div class="toggle_container">' . do_shortcode($content) . '</div>';
}
add_shortcode('toggle', 'toggle_shortcode');

